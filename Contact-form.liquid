{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-contact-form-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 40px;
    padding: 40px;
    background-color: {{ block.settings.background_color }};
    border-radius: {{ block.settings.border_radius }}px;
    max-width: {{ block.settings.max_width }}px;
    margin: 0 auto;
    width: 100%;
  }

  .ai-contact-form__left-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 20px;
  }

  .ai-contact-form__right-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 24px;
  }

  .ai-contact-form__heading-{{ ai_gen_id }} {
    color: {{ block.settings.heading_color }};
    font-size: {{ block.settings.heading_size }}px;
    font-weight: 600;
    margin: 0;
  }

  .ai-contact-form__form-heading-{{ ai_gen_id }} {
    color: {{ block.settings.form_heading_color }};
    font-size: {{ block.settings.form_heading_size }}px;
    font-weight: 600;
    margin: 0;
  }

  .ai-contact-form__text-{{ ai_gen_id }} {
    color: {{ block.settings.text_color }};
    font-size: {{ block.settings.text_size }}px;
    line-height: 1.6;
    margin: 0;
  }

  .ai-contact-form__field-group-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 8px;
  }

  .ai-contact-form__label-{{ ai_gen_id }} {
    color: {{ block.settings.label_color }};
    font-size: {{ block.settings.label_size }}px;
    font-weight: 500;
  }

  .ai-contact-form__input-{{ ai_gen_id }},
  .ai-contact-form__textarea-{{ ai_gen_id }} {
    padding: {{ block.settings.input_padding_vertical }}px {{ block.settings.input_padding_horizontal }}px;
    border: 1px solid {{ block.settings.input_border_color }};
    border-radius: {{ block.settings.input_border_radius }}px;
    font-size: {{ block.settings.input_text_size }}px;
    background-color: {{ block.settings.input_background_color }};
    color: {{ block.settings.input_text_color }};width: 100%;
    box-sizing: border-box;min-height: {{ block.settings.input_min_height }}px;
  }

  .ai-contact-form__input-{{ ai_gen_id }}:focus,
  .ai-contact-form__textarea-{{ ai_gen_id }}:focus {
    outline: none;
    border-color: {{ block.settings.input_focus_border_color }};
  }

  .ai-contact-form__textarea-{{ ai_gen_id }} {
    min-height: {{ block.settings.textarea_min_height }}px;
    resize: vertical;
  }

  .ai-contact-form__file-wrapper-{{ ai_gen_id }} {
    position: relative;
    display: inline-block;
    width: 100%;
  }

  .ai-contact-form__file-input-{{ ai_gen_id }} {
    position: absolute;
    opacity: 0;
    width: 100%;
    height: 100%;
    cursor: pointer;
  }

  .ai-contact-form__file-label-{{ ai_gen_id }} {
    display: flex;
    align-items: center;
    gap: 12px;
    padding: {{ block.settings.input_padding_vertical }}px {{ block.settings.input_padding_horizontal }}px;
    border: 2px dashed {{ block.settings.input_border_color }};
    border-radius: {{ block.settings.input_border_radius }}px;
    background-color: {{ block.settings.input_background_color }};
    cursor: pointer;
    transition: border-color 0.3s ease;
    color: {{ block.settings.text_color }};
    font-size: {{ block.settings.input_text_size }}px;min-height: {{ block.settings.input_min_height }}px;box-sizing: border-box;
  }

  .ai-contact-form__file-label-{{ ai_gen_id }}:hover {
    border-color: {{ block.settings.input_focus_border_color }};
  }

  .ai-contact-form__upload-icon-{{ ai_gen_id }} {
    width: 20px;
    height: 20px;
    color: {{ block.settings.icon_color }};
    flex-shrink: 0;
  }

  .ai-contact-form__custom-upload-icon-{{ ai_gen_id }} {
    width: 20px;
    height: 20px;
    object-fit: contain;
    flex-shrink: 0;
  }

  .ai-contact-form__button-{{ ai_gen_id }} {
    padding: 16px 32px;
    background-color: {{ block.settings.button_color }};
    color: {{ block.settings.button_text_color }};
    border: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    font-size: {{ block.settings.button_text_size }}px;
    font-weight: 600;
    cursor: pointer;
    transition: background-color 0.3s ease;
    align-self: flex-start;
  }

  .ai-contact-form__button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_color }};
  }

  .ai-contact-form__contact-info-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 20px;
  }

  .ai-contact-form__contact-item-{{ ai_gen_id }} {
    display: flex;
    align-items: flex-start;
    gap: 12px;
  }

  .ai-contact-form__contact-icon-{{ ai_gen_id }} {
    width: 24px;
    height: 24px;
    flex-shrink: 0;color: {{ block.settings.icon_color }};
    margin-top: 2px;
  }

  .ai-contact-form__custom-contact-icon-{{ ai_gen_id }} {
    width: 24px;
    height: 24px;
    object-fit: contain;
    flex-shrink: 0;
    margin-top: 2px;
  }

  .ai-contact-form__contact-text-{{ ai_gen_id }} {
    color: {{ block.settings.text_color }};
    font-size: {{ block.settings.contact_text_size }}px;
    line-height: 1.5;
  }

  .ai-contact-form__contact-link-{{ ai_gen_id }} {
    color: {{ block.settings.link_color }};
    text-decoration: none;
  }

  .ai-contact-form__contact-link-{{ ai_gen_id }}:hover {
    text-decoration: underline;
  }

  .ai-contact-form__success-{{ ai_gen_id }} {
    color: {{ block.settings.success_color }};
    background-color: {{ block.settings.success_background_color }};
    padding: 12px 16px;
    border-radius: {{ block.settings.input_border_radius }}px;
    margin-top: 16px;
  }

  .ai-contact-form__error-{{ ai_gen_id }} {
    color: {{ block.settings.error_color }};
    background-color: {{ block.settings.error_background_color }};
    padding: 12px 16px;
    border-radius: {{ block.settings.input_border_radius }}px;
    margin-top: 16px;
  }

  @media screen and (max-width: 768px) {
    .ai-contact-form-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 32px;
      padding: 24px;
    }

    .ai-contact-form__right-{{ ai_gen_id }} {
      order: -1;
    }
  }

  {% if block.settings.custom_css != blank %}
    {{ block.settings.custom_css }}
  {% endif %}
{% endstyle %}<contact-form-{{ ai_gen_id }}
  class="ai-contact-form-{{ ai_gen_id }}"
  {{ block.shopify_attributes }}
>
  <div class="ai-contact-form__left-{{ ai_gen_id }}">
    {%- if block.settings.form_heading != blank -%}
      <h2 class="ai-contact-form__form-heading-{{ ai_gen_id }}">
        {{ block.settings.form_heading }}
      </h2>
    {%- endif -%}

    {%- form 'contact' -%}
      <div class="ai-contact-form__field-group-{{ ai_gen_id }}">
        <label class="ai-contact-form__label-{{ ai_gen_id }}" for="contact-name-{{ ai_gen_id }}">
          Name *
        </label>
        <input
          type="text"
          id="contact-name-{{ ai_gen_id }}"
          name="contact[name]"
          class="ai-contact-form__input-{{ ai_gen_id }}"
          value="{{ form.name }}"
          required
          aria-required="true"
        >
      </div>

      <div class="ai-contact-form__field-group-{{ ai_gen_id }}">
        <label class="ai-contact-form__label-{{ ai_gen_id }}" for="contact-phone-{{ ai_gen_id }}">
          Phone Number
        </label>
        <input
          type="tel"
          id="contact-phone-{{ ai_gen_id }}"
          name="contact[phone]"
          class="ai-contact-form__input-{{ ai_gen_id }}"
          value="{{ form.phone }}"
        >
      </div>

      <div class="ai-contact-form__field-group-{{ ai_gen_id }}">
        <label class="ai-contact-form__label-{{ ai_gen_id }}" for="contact-email-{{ ai_gen_id }}">
          Email *
        </label>
        <input
          type="email"
          id="contact-email-{{ ai_gen_id }}"
          name="contact[email]"
          class="ai-contact-form__input-{{ ai_gen_id }}"
          value="{{ form.email }}"
          required
          aria-required="true"
          autocomplete="email"
        >
      </div>

      <div class="ai-contact-form__field-group-{{ ai_gen_id }}">
        <label class="ai-contact-form__label-{{ ai_gen_id }}" for="contact-comment-{{ ai_gen_id }}">
          Comment
        </label>
        <textarea
          id="contact-comment-{{ ai_gen_id }}"
          name="contact[body]"
          class="ai-contact-form__textarea-{{ ai_gen_id }}"
          placeholder="Tell us how we can help you..."
        >{{ form.body }}</textarea>
      </div>

      <div class="ai-contact-form__field-group-{{ ai_gen_id }}">
        <label class="ai-contact-form__label-{{ ai_gen_id }}">
          Upload Image
        </label>
        <div class="ai-contact-form__file-wrapper-{{ ai_gen_id }}">
          <input
            type="file"
            id="contact-file-{{ ai_gen_id }}"
            name="contact[attachment]"
            class="ai-contact-form__file-input-{{ ai_gen_id }}"
            accept="image/*">
          <label for="contact-file-{{ ai_gen_id }}" class="ai-contact-form__file-label-{{ ai_gen_id }}">
            {% if block.settings.upload_icon %}
              <img
                src="{{ block.settings.upload_icon | image_url: width: 40}}"
                alt="Upload icon"
                class="ai-contact-form__custom-upload-icon-{{ ai_gen_id }}"
                loading="lazy"
              >
            {% else %}
              <svg class="ai-contact-form__upload-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                <path d="M21 15v4a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2v-4"/>
                <polyline points="7,10 12,15 17,10"/>
                <line x1="12" y1="15" x2="12" y2="3"/>
              </svg>
            {% endif %}
            <span class="ai-contact-form__file-text-{{ ai_gen_id }}">Click to upload an image</span>
          </label>
        </div>
      </div>

      <button type="submit" class="ai-contact-form__button-{{ ai_gen_id }}">
        {{ block.settings.submit_button_text }}
      </button>

      {%- if form.posted_successfully? -%}
        <div class="ai-contact-form__success-{{ ai_gen_id }}">
          {{ block.settings.success_message }}
        </div>
      {%- endif -%}

      {%- if form.errors -%}
        <div class="ai-contact-form__error-{{ ai_gen_id }}">
          {{ form.errors | default_errors }}
        </div>
      {%- endif -%}
    {%- endform -%}
  </div>

  <div class="ai-contact-form__right-{{ ai_gen_id }}">
    <h2 class="ai-contact-form__heading-{{ ai_gen_id }}">
      {{ block.settings.heading }}
    </h2>

    {%- if block.settings.description != blank -%}
      <div class="ai-contact-form__text-{{ ai_gen_id }}">
        {{ block.settings.description }}
      </div>
    {%- endif -%}

    <div class="ai-contact-form__contact-info-{{ ai_gen_id }}">
      {%- if block.settings.whatsapp_number != blank -%}
        <div class="ai-contact-form__contact-item-{{ ai_gen_id }}">
          {% if block.settings.whatsapp_icon %}
            <img
              src="{{ block.settings.whatsapp_icon | image_url: width: 48}}"
              alt="WhatsApp icon"
              class="ai-contact-form__custom-contact-icon-{{ ai_gen_id }}"
              loading="lazy"
            >
          {% else %}
            <svg class="ai-contact-form__contact-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="currentColor">
              <path d="M17.472 14.382c-.297-.149-1.758-.867-2.03-.967-.273-.099-.471-.148-.67.15-.197.297-.767.966-.94 1.164-.173.199-.347.223-.644.075-.297-.15-1.255-.463-2.39-1.475-.883-.788-1.48-1.761-1.653-2.059-.173-.297-.018-.458.13-.606.134-.133.298-.347.446-.52.149-.174.198-.298.298-.497.099-.198.05-.371-.025-.52-.075-.149-.669-1.612-.916-2.207-.242-.579-.487-.5-.669-.51-.173-.008-.371-.01-.57-.01-.198 0-.52.074-.792.372-.272.297-1.04 1.016-1.04 2.479 0 1.462 1.065 2.875 1.213 3.074.149.198 2.096 3.2 5.077 4.487.709.306 1.262.489 1.694.625.712.227 1.36.195 1.871.118.571-.085 1.758-.719 2.006-1.413.248-.694.248-1.289.173-1.413-.074-.124-.272-.198-.57-.347m-5.421 7.403h-.004a9.87 9.87 0 01-5.031-1.378l-.361-.214-3.741.982.998-3.648-.235-.374a9.86 9.86 0 01-1.51-5.26c.001-5.45 4.436-9.884 9.888-9.884 2.64 0 5.122 1.03 6.988 2.898a9.825 9.825 0 012.893 6.994c-.003 5.45-4.437 9.884-9.885 9.884m8.413-18.297A11.815 11.815 0 0012.05 0C5.495 0 .16 5.335.157 11.892c0 2.096.547 4.142 1.588 5.945L.057 24l6.305-1.654a11.882 11.882 0 005.683 1.448h.005c6.554 0 11.890-5.335 11.893-11.893A11.821 11.821 0 0020.4653.63z"/>
            </svg>
          {% endif %}
          <div class="ai-contact-form__contact-text-{{ ai_gen_id }}">
            <a href="https://wa.me/{{ block.settings.whatsapp_number | remove: '+' | remove: ' ' | remove: '-' }}" class="ai-contact-form__contact-link-{{ ai_gen_id }}">
              {{ block.settings.whatsapp_number }}
            </a>
          </div>
        </div>
      {%- endif -%}

      {%- if block.settings.email_address != blank -%}
        <div class="ai-contact-form__contact-item-{{ ai_gen_id }}">
          {% if block.settings.email_icon %}
            <img
              src="{{ block.settings.email_icon | image_url: width: 48}}"
              alt="Email icon"
              class="ai-contact-form__custom-contact-icon-{{ ai_gen_id }}"
              loading="lazy"
            >
          {% else %}
            <svg class="ai-contact-form__contact-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <path d="M4 4h16c1.1 0 2 .9 2 2v12c0 1.1-.9 2-2 2H4c-1.1 0-2-.9-2-2V6c0-1.1.9-2 2-2z"/>
              <polyline points="22,6 12,13 2,6"/>
            </svg>
          {% endif %}
          <div class="ai-contact-form__contact-text-{{ ai_gen_id }}">
            <a href="mailto:{{ block.settings.email_address }}" class="ai-contact-form__contact-link-{{ ai_gen_id }}">
              {{ block.settings.email_address }}
            </a>
          </div>
        </div>
      {%- endif -%}

      {%- if block.settings.address != blank -%}
        <div class="ai-contact-form__contact-item-{{ ai_gen_id }}">
          {% if block.settings.location_icon %}
            <img
              src="{{ block.settings.location_icon | image_url: width: 48 }}"
              alt="Location icon"
              class="ai-contact-form__custom-contact-icon-{{ ai_gen_id }}"
              loading="lazy"
            >
          {% else %}
            <svg class="ai-contact-form__contact-icon-{{ ai_gen_id }}" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
              <path d="M21 10c0 7-9 13-9 13s-9-6-9-13a9 9 0 0 1 18 0z"/>
              <circle cx="12" cy="10" r="3"/>
            </svg>
          {% endif %}
          <div class="ai-contact-form__contact-text-{{ ai_gen_id }}">
            {{ block.settings.address }}
          </div>
        </div>
      {%- endif -%}
    </div>
  </div>
</contact-form-{{ ai_gen_id }}><script>
  (function() {
    class ContactForm{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.fileInput = this.querySelector('#contact-file-{{ ai_gen_id }}');
        this.fileText = this.querySelector('.ai-contact-form__file-text-{{ ai_gen_id }}');
        this.setupFileUpload();
      }

      setupFileUpload() {
        if (this.fileInput && this.fileText) {
          this.fileInput.addEventListener('change', (event) => {
            const file = event.target.files[0];
            if (file) {
              this.fileText.textContent = `Selected: ${file.name}`;} else {
              this.fileText.textContent = 'Click to upload an image';
            }
          });
        }
      }
    }

    customElements.define('contact-form-{{ ai_gen_id }}', ContactForm{{ ai_gen_id }});
  })();
</script>

